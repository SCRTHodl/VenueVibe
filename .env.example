# CLIENT-SIDE VARIABLES (with VITE_ prefix - WILL be exposed in the browser)
# Main Supabase Configuration for client operations
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# SERVER-SIDE VARIABLES (no VITE_ prefix - will NOT be exposed in browser)
# These are only for Supabase Edge Functions or your backend server
# IMPORTANT: Do not use these variables in your frontend code!

# DEPRECATED: Do not use these in new code
# Kept only for backward compatibility
# VITE_SUPABASE_SERVICE_ROLE_KEY=DO_NOT_USE_IN_CLIENT_CODE
# VITE_SUPABASE_SERVICE_KEY=DO_NOT_USE_IN_CLIENT_CODE

# Token Economy Schema Configuration
VITE_TOKEN_ECONOMY_SCHEMA=token_economy

VITE_MAPBOX_TOKEN=

# Token Economy Configuration
VITE_TOKEN_CREATION_BASE_REWARD=5
VITE_TOKEN_LOCATION_BONUS=2
VITE_TOKEN_VIDEO_BONUS=3
VITE_TOKEN_TAG_BONUS=1

# Stripe Configuration
# NFT Features Configuration
VITE_NFT_ENABLED=false
VITE_NFT_MINIMUM_TOKENS=100

# Feature Flags
VITE_ENABLE_TOKEN_ECONOMY=true
VITE_ENABLE_PREMIUM_CONTENT=true
VITE_ENABLE_VENUE_POSTS=true
