# VenueVibe Environment Variables Example
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit .env files with real credentials to version control

# Main Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key - DO NOT EXPOSE IN CLIENT CODE
# Used for admin functions and token economy operations
VITE_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Mapbox Configuration
VITE_MAPBOX_TOKEN=your-mapbox-token

# Stripe Configuration
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key

# Token Economy Schema Configuration
VITE_TOKEN_ECONOMY_SCHEMA=token_economy

# Token Economy Rewards Configuration
VITE_TOKEN_CREATION_BASE_REWARD=5
VITE_TOKEN_LOCATION_BONUS=2
VITE_TOKEN_VIDEO_BONUS=3
VITE_TOKEN_TAG_BONUS=1
VITE_TOKEN_COMMENT_REWARD=1
VITE_TOKEN_PREMIUM_CONTENT_COST=10

# NFT Features Configuration
VITE_NFT_ENABLED=true
VITE_NFT_MINIMUM_TOKENS=100

# Feature Flags
VITE_ENABLE_TOKEN_ECONOMY=true
VITE_ENABLE_PREMIUM_CONTENT=true
VITE_ENABLE_VENUE_POSTS=true

# API Keys for Third-Party Services (if needed)
VITE_MAPS_API_KEY=your-maps-api-key-here
VITE_AI_SERVICE_KEY=your-ai-service-key-here